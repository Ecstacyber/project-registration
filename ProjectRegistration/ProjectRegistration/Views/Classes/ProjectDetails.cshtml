                alert("Thêm thành viên vào đồ án thất bại%0D%0AĐã hết thời gian đăng ký");
            }
            if (msg == 'MemberAddedToProject') {
                alert("Thêm thành viên vào đồ án thành công");
            }
            if (msg == 'MemberNotAddedToProject') {
